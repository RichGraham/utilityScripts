#A mac-specific bash profile
. /sw/bin/init.sh
export PATH=$PATH:~/Dropbox/bin/:/usr/texbin/
alias amacs='open -a /Applications/Aquamacs.app'
alias emacs='open -a /Applications/Aquamacs.app'
alias excel='open -a /Applications/Microsoft\ Excel.app/'
alias top='top -o cpu'

my_openiPython() {
ipython notebook $1 &
}
alias ipyth=my_openiPython



export LSCOLORS=ExFxCxDxBxegedabagacad