#A mac-specific bash profile
. /sw/bin/init.sh
export PATH=$PATH:~/Dropbox/bin/:/usr/texbin/:/Library/TeX/Distributions/.DefaultTeX/Contents/Programs/texbin/
export LIBRARY_PATH="$LIBRARY_PATH:/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/usr/lib"
alias amacs='open -a /Applications/Aquamacs.app'
alias excel='open -a /Applications/Microsoft\ Office\ 2011/Microsoft\ Excel.app/'
alias top='top -o cpu'

my_openiPython() {
ipython notebook $1 &
}
alias ipyth=my_openiPython



export LSCOLORS=ExFxCxDxBxegedabagacad
